ORG 0000H
MOV R0, #40H       
MOV R1, #40H      

SCAN_LOOP:
    MOV A, @R0     
    CJNE A, #0FFH, KEEP_DATA 
    SJMP NEXT_BYTE 
KEEP_DATA:
    MOV @R1, A     
    INC R1        

NEXT_BYTE:
    INC R0          
    CJNE R0, #60H, SCAN_LOOP 
ZERO_FILL:
    MOV A, R1
    CJNE A, #60H, FILL_NOW
    SJMP DONE

FILL_NOW:
    MOV @R1, #00H  
    INC R1
    SJMP ZERO_FILL

DONE:
    SJMP $        
END
